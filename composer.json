{
  "name": "ncjoes/pdf-suite",
  "description": "A high level PHP wrapper over [PopplerPhp](https://github.com/ncjoes/poppler-php) for PDF content extraction and conversion using Poppler utils",
  "keywords": ["Poppler-utils", "PDF Converter", "PDF", "PDF to HTML", "PDF to SVG", "PDF to JPG", "PDF to PNG", "PDF to PostScript"],
  "license": "MIT",
  "authors": [
    {
      "name": "Chukwuemeka Nwobodo",
      "email": "jc.nwobodo@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^5.6 || ^7.0",
    "ncjoes/poppler-php": "dev-master",
    "illuminate/filesystem": "5.3.*",
    "illuminate/support": "v5.3.*"
  },
  "require-dev":{
    "phpunit/phpunit":"5.5.*"
  },

  "autoload": {
    "psr-4": {
      "NcJoes\\PdfSuite\\": "src/PdfSuite"
    }
  }
}
